{
  "orgId": "vacco",
  "internalRepo": {
    "id": "GithubPackages",
    "url": "https://maven.pkg.github.com/vaccovecrana/common-build/",
    "usernameEnvProperty": "GH_USER",
    "passwordEnvProperty": "GH_TOKEN"
  },
  "snapshotsRepo": {
    "id": "SonatypeOSSSnapshots",
    "url": "https://oss.sonatype.org/content/repositories/snapshots/",
    "usernameEnvProperty": "SONATYPE_USER",
    "passwordEnvProperty": "SONATYPE_PASSWORD"
  },
  "releasesRepo": {
    "id": "SonatypeOSSStaging",
    "url": "https://oss.sonatype.org/service/local/staging/deploy/maven2/",
    "usernameEnvProperty": "SONATYPE_USER",
    "passwordEnvProperty": "SONATYPE_PASSWORD"
  },
  "publishing": {
    "id": "Vacco",
    "devId": "vacco",
    "devContact": "Vaccove Crana, LLC.",
    "devEmail": "devops@vacco.io",
    "mavenSigningKeyEnvProperty": "MAVEN_SIGNING_PRV"
  },
  "devConfig": {
    "jdkDistribution": "https://api.adoptopenjdk.net/v3/binary/latest/11/ga/linux/x64/jdk/hotspot/normal/adoptopenjdk",
    "pmdRulesUrl": "file:./src/test/resources/org-config/pmd-config.xml",
    "dependencyExcludedGroups": ["net.sourceforge.pmd"],
    "versions": {
      "gradle": "gradle-6.8.3",
      "j8Spec": "io.github.j8spec:j8spec:3.0.0",
      "pmd": "6.27.0"
    }
  }
}
